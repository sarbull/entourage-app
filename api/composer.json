{
    "name": "laravel/lumen",
    "description": "Entourage",
    "keywords": [],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/lumen-framework": "5.1.*",
        "vlucas/phpdotenv": "~1.0",
        "tymon/jwt-auth": "^0.5.5",
        "basicit/lumen-vendor-publish": "^1.0",
        "illuminate/routing": "^5.1",
        "league/fractal": "^0.12.0",
        "vluzrmos/tinker": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "fzaninotto/faker": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/",
            "app/models"
        ],
        "files": [
          "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
